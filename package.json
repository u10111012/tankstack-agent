{
  "name": "tankstack-agent",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm dev:frontend",
    "dev:frontend": "cd frontend && vite dev --port 3000",
    "dev:backend": "cd backend && wrangler dev --config wrangler.jsonc",
    "build:widget": "cd frontend && vite build --config vite.config.widget.ts && cp -r widget-dist/* public/ && rm -rf widget-dist",
    "build": "pnpm build:widget && cd frontend && vite build",
    "serve": "cd frontend && vite preview",
    "test": "cd frontend && vitest run",
    "db:generate": "cd backend && drizzle-kit generate",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "deploy": "wrangler deploy"
  },
